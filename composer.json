{
  "name": "baks-dev/yandex-market-orders",
  "description": "Модуль заказов Yandex Market",
  "homepage": "https://github.com/baks-dev/yandex-market-orders",
  "keywords": [
    "symfony",
    "bundle",
    "library",
    "module",
    "plugin",
    "yandex",
    "market",
    "orders",
    "baksdev"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Yandex\\Market\\Orders\\": ""
    }
  },
  "scripts": {
    "auto-scripts": {
      "baks:users-profile-type:yandex-market": "symfony-cmd",
      "baks:payment:yandex-market": "symfony-cmd",
      "baks:delivery:yandex-market": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
